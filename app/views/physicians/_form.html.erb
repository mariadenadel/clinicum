<%= simple_form_for @physician do |form| %>
  <%= form.input :first_name %>
  <%= form.input :last_name %>
  <%= form.input :phone_number %>
  <%= form.input :level, collection: Physician::LEVELS %>
  <br>
  <%= form.button :submit, class: "btn btn-success" %>
  <p>
    <%= link_to "< Back", :back %>
  </p>
<% end %>
